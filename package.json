  "type": "module",
  "scripts": {
    "build": "pnpm run build:cache && cross-env NODE_OPTIONS=--no-deprecation next build && serwist build --config serwist.config.js",
    "build:cache": "esbuild src/cache-handlers/default.ts --bundle --platform=node --format=cjs --outfile=src/cache-handlers/default.cjs --external:next --external:ioredis --external:node:crypto --external:node:fs --external:node:path --external:node:fs/promises --external:@opentelemetry/api",
    "db:pull": "bash src/scripts/db/pull.sh",
    "db:push-dev": "bash src/scripts/db/push-dev.sh",
    "db:tunnel-dev": "bash src/scripts/db/tunnel.sh dev",
    "db:tunnel-konekta": "bash src/scripts/db/tunnel.sh konekta",
    "db:tunnel-prod": "bash src/scripts/db/tunnel.sh prod",
    "dev": "docker compose up --build --scale payload=0 -d && cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next/* && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "(tsc --noemit --project ./tsconfig.json); eslint -c eslint.config.mjs --fix --quiet --cache .",
    "lint:check": "(tsc --noemit --project ./tsconfig.json); eslint -c eslint.config.mjs .",
    "lint:warnings": "(tsc --noemit --project ./tsconfig.json); eslint -c eslint.config.mjs  --fix .",
    "lint-staged": "lint-staged",
    "prettier": "prettier --config-path .prettierrc.json --write --cache --experimental-cli .",
    "prettier:check": "prettier  --config .prettierrc.json --check .",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "jest"
  },
  "lint-staged": {
    "**/package.json": "sort-package-json",
    "*.{md,mdx,yml,json}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
